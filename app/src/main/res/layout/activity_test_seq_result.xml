                        android:text="@string/_0"/>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorDarkGrey"/>

                <LinearLayout
                    style="@style/resultTableLinearLayout">

                    <TextView
                        android:id="@+id/txt16"
                        style="@style/center16spBlackText"
                        android:minWidth="80dp"
                        android:gravity="center"
                        android:text="@string/avg"/>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/colorDarkGrey"/>

                    <TextView
                        android:id="@+id/txtAvgTime"
                        style="@style/resultTableItemText"
                        android:text="@string/_0"/>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorDarkGrey"/>

            </LinearLayout>

            <View style="@style/space_verti"/>

            <Button
                android:id="@+id/btnBack"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@drawable/s_btn_plain_dark2"
                android:text="처음으로 돌아가기"
                android:textColor="#fff"
                android:textSize="14sp"
                android:layout_margin="10dp"/>

        </LinearLayout>


        <include layout="@layout/sliding_menu" />
    </FrameLayout>

