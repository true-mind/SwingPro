<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorCommonBack">

    <include layout="@layout/common_head"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_marginTop="5dp"
                android:padding="10dp"
                style="@style/m_none_fragItemBase"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtTitle1"
                    android:layout_marginBottom="10dp"
                    style="@style/plain20spBlackText"
                    android:text="결과 - 버튼별"/>


                <LinearLayout
                    style="@style/resultTableLinearLayout">

                    <TextView
                        android:id="@+id/txt1"
                        style="@style/center16spBlackText"
                        android:minWidth="80dp"
                        android:gravity="center"
                        android:text="@string/ms_unit"/>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/colorDarkGrey"/>

                    <TextView
                        android:id="@+id/txt2"
                        style="@style/resultTableItemText"
                        android:text="@string/no1"/>

                    <TextView
                        android:id="@+id/txt3"
                        style="@style/resultTableItemText"
                        android:text="@string/no2"/>

                    <TextView
                        android:id="@+id/txt4"
                        style="@style/resultTableItemText"
                        android:text="@string/no3"/>

                    <TextView
                        android:id="@+id/txt5"
                        style="@style/resultTableItemText"
                        android:text="@string/no4"/>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorDarkGrey"/>

                <LinearLayout
                    style="@style/resultTableLinearLayout">

                    <TextView
                        android:id="@+id/txt6"
                        style="@style/center16spBlackText"
                        android:minWidth="80dp"
                        android:gravity="center"
                        android:text="@string/first_try"/>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/colorDarkGrey"/>

                    <TextView
                        android:id="@+id/txtBtn1Time1"
                        style="@style/resultTableItemText"
                        android:text="@string/_0"/>

                    <TextView
                        android:id="@+id/txtBtn1Time2"
                        style="@style/resultTableItemText"
                        android:text="@string/_0"/>

                    <TextView
                        android:id="@+id/txtBtn1Time3"
                        style="@style/resultTableItemText"
                        android:text="@string/_0"/>

                    <TextView
                        android:id="@+id/txtBtn1Time4"
                        style="@style/resultTableItemText"
                        android:text="@string/_0"/>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorDarkGrey"/>

                <LinearLayout
                    style="@style/resultTableLinearLayout">

                    <TextView
                        android:id="@+id/txt7"
                        style="@style/center16spBlackText"
                        android:minWidth="80dp"
                        android:gravity="center"
                        android:text="@string/second_try"/>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/colorDarkGrey"/>

                    <TextView
                        android:id="@+id/txtBtn2Time1"
                        style="@style/resultTableItemText"
                        android:text="@string/_0"/>

                    <TextView
                        android:id="@+id/txtBtn2Time2"
                        style="@style/resultTableItemText"
                        android:text="@string/_0"/>

                    <TextView
                        android:id="@+id/txtBtn2Time3"
                        style="@style/resultTableItemText"
